part1 : Nat := filter level-ok? input |> length;

level-ok? : List Int -> Bool
  | xs := monotonic? xs && max-difference xs < 4 && min-difference xs > 0;

monotonic? {A} {{Ord A}} : List A -> Bool
  | xs := monotonically-increasing? xs || monotonically-decreasing? xs;

monotonically-increasing? {A} {{Ord A}} : List A -> Bool
  | xs := monotonically-gen (<) xs;

monotonically-decreasing? {A} {{Ord A}} : List A -> Bool
  | xs := monotonically-gen (>) xs;

max-difference : List Int -> Nat
  | xs := difference-gen (>) xs;

min-difference : List Int -> Nat
  | xs := difference-gen (<=) xs;

----------------------------------------------------------------------
-- Helper general functions
----------------------------------------------------------------------

monotonically-gen {A} {{Ord A}} : (A -> A -> Bool) -> List A -> Bool
  | _ [] := true
  | pred (x :: xs) :=
    for (previous, res := x, true) (x in xs) {
        (,) x (pred previous x && res)
      }
      |> snd;

difference-gen : (Nat -> Nat -> Bool) -> List Int -> Nat
  | _ [] := 0
  | _ (x :: []) := 0
  | pred (x :: y :: xs) :=
    for (acc, last := abs (y - x), y) (x in xs) {
        let
          diff := abs (last - x);
        in if
          | pred diff acc := diff, x
          | else := acc, x
      }
      |> fst;
